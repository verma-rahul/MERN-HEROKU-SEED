    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "express": "^4.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/verma-rahul/MERN-HEROKU-SEED"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
